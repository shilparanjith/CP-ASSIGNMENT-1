//C progran to find values of sinx and cosx
#include<stdio.h>
#include<math.h>
void main()
    {
          float angle,x;
          printf("\nEnter angle : ");
          scanf("%f",&angle);
          x=(angle*((22/7)/180));
          printf("\nsin(%f)=%f",angle,sin(x));
          printf("\ncos(%f)=%f",angle,cos(x));
    }
          
          
          
