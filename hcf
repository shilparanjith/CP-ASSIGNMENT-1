#include<stdio.h>
void main()
  {
      int n1,n2,hcf;
      printf("Enter two numbers : ");
      scanf("%d%d",&n1,&n2);
      for(int i=0;i<=n1&&i<=n2;i++);
      {
          if(n1%i==0&&n2%i==0)
          {
            hcf=i;
          }
      }
      printf("\nThe hcf of the numbers %d and %d = %d",n1,n2,hcf);
  }
