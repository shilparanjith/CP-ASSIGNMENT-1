#include<stdio.h>
void main()
  {
      int n,a=0,b=1,c;
      printf("Enter the limit :");
      scanf("%d",&n);
      printf("The fibanoci series is :\t%d\t%d",a,b);
      for(int i=2;i<n;i++)
        {
            c=a+b;
            printf("\t%d",c);
            a=b;
            b=c;
        }
  }
      
